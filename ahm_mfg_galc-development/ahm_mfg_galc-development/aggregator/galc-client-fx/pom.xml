	<build>
		<plugins>
			<plugin>
   				<groupId>org.apache.maven.plugins</groupId>
   				<artifactId>maven-resources-plugin</artifactId>
				<executions>
   					<execution>
       					<id>copy-resource-files</id>
       					<phase>validate</phase>
       					<goals>
           					<goal>copy-resources</goal>
       					</goals>
       					<configuration>
           					<outputDirectory>${project.build.directory}/resource</outputDirectory>
           					<resources>
               					<resource>
                   					<filtering>true</filtering>
                					<directory>../../client-resource/src/main/resources/resource</directory>
               					</resource>
           					</resources>
       					</configuration>
  					</execution>
				</executions>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-shade-plugin</artifactId>
				<executions>
					<execution>
						<goals>
							<goal>shade</goal>
						</goals>
						<configuration>
							<createDependencyReducedPom>false</createDependencyReducedPom>
							<filters>
								<filter>
									<artifact>*:*</artifact>
									<excludes>
										<exclude>META-INF/*.SF</exclude>
										<exclude>META-INF/*.DSA</exclude>
										<exclude>META-INF/*.RSA</exclude>
										<exclude>**/Log4j2Plugins.dat</exclude>
									</excludes>
								</filter>
							</filters>
							<transformers>
								<transformer implementation="org.apache.maven.plugins.shade.resource.ManifestResourceTransformer">
									<manifestEntries>
										<Class-Path>.</Class-Path>
										<mainClass>com.honda.galc.client.ClientMainFx</mainClass>
									</manifestEntries>
								</transformer>
								<transformer implementation="org.apache.maven.plugins.shade.resource.AppendingTransformer">
									<resource>META-INF/spring.handlers</resource>
								</transformer>
								<transformer implementation="org.apache.maven.plugins.shade.resource.AppendingTransformer">
									<resource>META-INF/spring.schemas</resource>
								</transformer>
							</transformers>
						</configuration>
					</execution>
				</executions>
			</plugin>
			
			<plugin>
   				<groupId>org.apache.maven.plugins</groupId>
   				<artifactId>maven-jarsigner-plugin</artifactId>
			</plugin>
		</plugins>
	</build>

