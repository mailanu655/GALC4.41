							<role-name>ViewAdmin</role-name>
						</security-role>
						<security-role id="SecurityRole_EditProcessPoint">
							<description>Edit Process Point Data</description>
							<role-name>EditProcessPoint</role-name>
						</security-role>
						<security-role id="SecurityRole_ViewProcessPoint">
							<description>View Process Point Data</description>
							<role-name>ViewProcessPoint</role-name>
						</security-role>
						<security-role id="SecurityRole_ViewUsers">
							<description>View GALC Users</description>
							<role-name>ViewUsers</role-name>
						</security-role>
						<security-role id="SecurityRole_EditUsers">
							<description>Modify GALC Application Users</description>
							<role-name>EditUsers</role-name>
						</security-role>
						<security-role id="SecurityRole_EditTerminal">
							<description>Edit Terminal Definitions</description>
							<role-name>EditTerminal</role-name>
						</security-role>
						<security-role id="SecurityRole_ViewTerminal">
							<description>View Terminal Settings</description>
							<role-name>ViewTerminal</role-name>
						</security-role>
						<security-role id="SecurityRole_EditDevice">
