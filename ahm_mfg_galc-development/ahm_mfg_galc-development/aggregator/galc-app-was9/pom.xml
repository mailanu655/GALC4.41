	<!-- module names are overridden so that they match with the names in the 
		existing application.xml -->

	<build>
		<finalName>${galc.build.prefix}_${galc.build.version}_app</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-ear-plugin</artifactId>
				<configuration>
					<generateApplicationXml>false</generateApplicationXml>
					<packagingExcludes>ibmconfig/**</packagingExcludes>
					<defaultLibBundleDir>lib/</defaultLibBundleDir>
					<outputFileNameMapping>@{artifactId}@-@{version}@@{dashClassifier?}@.@{extension}@</outputFileNameMapping>
					<skinnyWars>true</skinnyWars>
					<displayName>GALC New Application</displayName>
					<modules>
						<rarModule id="ConnectorModule_1378840475363">
							<groupId>com.honda.galc</groupId>
							<artifactId>jca-adaptor</artifactId>
							<bundleFileName>jca-adaptor.rar</bundleFileName>
						</rarModule>
						
						<ejbModule id="EjbModule_1378840475332">
							<groupId>com.honda.galc</groupId>
							<artifactId>jca-socket-ejb</artifactId>
							<bundleFileName>jca-socket-ejb.jar</bundleFileName>
						</ejbModule>
						
						<ejbModule id="EjbModule_1290866836243">
							<groupId>com.honda.galc</groupId>
							<artifactId>oif-service-ejb</artifactId>
							<bundleFileName>oif-service-ejb.jar</bundleFileName>
						</ejbModule>

						<webModule id="WebModule_1279797210744">
							<groupId>com.honda.galc</groupId>
							<artifactId>base-web</artifactId>
							<contextRoot>BaseWeb</contextRoot>
							<bundleFileName>base-web.war</bundleFileName>
						</webModule>

						<webModule id="WebModule_1288034335300">
							<groupId>com.honda.galc</groupId>
							<artifactId>base-web-start</artifactId>
							<contextRoot>BaseWebStart</contextRoot>
							<bundleFileName>base-web-start.war</bundleFileName>
						</webModule>

						<webModule id="WebModule_1288094812334">
							<groupId>com.honda.galc</groupId>
							<artifactId>config-service-web</artifactId>
							<contextRoot>ConfigService</contextRoot>
							<bundleFileName>config-service-web.war</bundleFileName>
						</webModule>

						<webModule id="WebModule_1373281495868">
							<groupId>com.honda.galc</groupId>
							<artifactId>rest-web</artifactId>
							<contextRoot>RestWeb</contextRoot>
							<bundleFileName>rest-web.war</bundleFileName>
						</webModule>

						<webModule id="WebModule_1290866787180">
							<groupId>com.honda.galc</groupId>
							<artifactId>oif-config-web</artifactId>
							<contextRoot>oifConfig</contextRoot>
							<bundleFileName>oif-config-web.war</bundleFileName>
						</webModule>

						<webModule id="WebModule_1514734754355">
							<groupId>com.honda.galc</groupId>
							<artifactId>handheld-web</artifactId>
							<contextRoot>Handheld</contextRoot>
							<bundleFileName>handheld-web.war</bundleFileName>
						</webModule>
					</modules>
					<security>
						<security-role id="SecurityRole_AllUsers">
						<description>All authenticated Users</description>
						<role-name>AllUsers</role-name>
						</security-role>
						<security-role id="SecurityRole_EditAdmin">
