    <bean id="entityManagerFactory" class="org.springframework.orm.jpa.LocalContainerEntityManagerFactoryBean">
		<property name="jpaVendorAdapter">
	         <bean class="org.springframework.orm.jpa.vendor.OpenJpaVendorAdapter">
		           <property name="showSql" value="true" />
	          </bean>
		</property>
		<property name="entityManagerInterface" value="com.ibm.websphere.persistence.WsJpaEntityManager" />
		<property name="persistenceProviderClass" value="com.ibm.websphere.persistence.PersistenceProviderImpl" />
		<property name="jpaProperties">
			<props>
				<prop key="openjpa.Log">DefaultLevel=ERROR</prop>
			</props>
		</property>
	</bean>
       
    <bean
        class="org.springframework.orm.jpa.support.PersistenceAnnotationBeanPostProcessor" />

    
