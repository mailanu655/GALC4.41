		</xs:annotation>
		<xs:restriction base="xs:unsignedInt" />
	</xs:simpleType>
	<xs:simpleType name="QW_part">
		<xs:annotation>
			<xs:documentation>Database part</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sciemetric:QW_StringFilterL">
			<xs:maxLength value="20" />
			<xs:minLength value="1" />
			<xs:whiteSpace value="collapse" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QW_precision">
		<xs:annotation>
			<xs:documentation>Database precision (from MSDN %[flags] [width] [.precision] [{h | l | I64 | L}]type )</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="10" />
			<xs:whiteSpace value="preserve" />
			<xs:pattern value="%(\-|\+|\s|#)?\p{N}*(\.\p{N}+)?(h|I|I64|L)?(c|C|d|i|o|u|x|X|e|E|f|g|G|n|p|s|S)" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QW_serialnumber">
		<xs:annotation>
			<xs:documentation>Database serialnumber</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sciemetric:QW_StringFilterSN_MN">
			<xs:minLength value="1" />
			<xs:maxLength value="64" />
			<xs:whiteSpace value="collapse" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QW_StringFilterD">
		<xs:annotation>
			<xs:documentation>Sciemetric String Filter for Descriptions</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9A-Za-z][\- !#$&amp;\(\)\*\+,\./0-9:;&lt;=&gt;\?@A-Z\[\\\]\^_a-z]*" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QW_StringFilterF">
		<xs:annotation>
			<xs:documentation>Sciemetric String Filter for Filenames</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[\- !#$%&amp;'\(\)\+,\.0-9=@A-Z\[\]\^_a-z\{\}~]*" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QW_StringFilterL">
		<xs:annotation>
			<xs:documentation>Sciemetric String Filter for Labels</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9A-Za-z][\-\(\)/0-9A-Z\[\\\]_a-z]*" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QW_StringFilterN">
		<xs:annotation>
			<xs:documentation>Sciemetric String Filter for Names</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9A-Za-z][\- \(\)/0-9A-Z\[\\\]_a-z]*" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QW_StringFilterSN_MN">
