	<xs:simpleType name="GUIDType">
		<xs:annotation>
			<xs:documentation>String containing a GUID</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="36" />
			<xs:whiteSpace value="collapse" />
			<xs:pattern value="[A-Z0-9]{8}-[A-Z0-9]{4}-[A-Z0-9]{4}-[A-Z0-9]{4}-[A-Z0-9]{12}" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ReferenceNumberType">
		<xs:annotation>
			<xs:documentation>Reference to binary format</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer" />
	</xs:simpleType>
	<xs:simpleType name="QW_label">
		<xs:annotation>
