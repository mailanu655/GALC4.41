<?xml version="1.0" encoding="utf-8" ?>
<!--Created with Liquid XML Studio Developer Edition (Trial) 8.1.0.2319 (http://www.liquid-technologies.com)-->
<xs:schema xmlns:sciemetric="http://www.sciemetric.com/namespace" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://www.sciemetric.com/namespace" version="3.10.68.1" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="root">
		<xs:annotation>
			<xs:documentation>This is the root of the file</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="single_part">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="information" minOccurs="0">
								<xs:annotation>
									<xs:documentation>This is all the information about the generation of the file. Some of the fields are useless but may be used in the future</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="source" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="version" type="xs:string" default="1">
														<xs:annotation>
															<xs:documentation>version of the application</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="date" type="sciemetric:QW_datetime">
														<xs:annotation>
															<xs:documentation>Generation date</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="application" type="xs:string">
														<xs:annotation>
															<xs:documentation>Name of the application that generated the file.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="database" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="host" type="xs:string">
														<xs:annotation>
															<xs:documentation>host of the database</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="database" type="xs:string">
														<xs:annotation>
															<xs:documentation>database catalog name</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="version" type="xs:string" default="1">
														<xs:annotation>
															<xs:documentation>Database version or database schema version</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="id" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>A unique identifier that identifies each database.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="part" type="sciemetric:PartType">
								<xs:annotation>
									<xs:documentation>This is the description of the part element</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:choice minOccurs="0">
								<xs:element name="sections" type="sciemetric:SectionsType">
									<xs:annotation>
										<xs:documentation>This is the data holder with all the information about the history of the part</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="stations" type="sciemetric:StationsType">
									<xs:annotation>
										<xs:documentation>If no section, we will have a list of stations</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:choice>
							<xs:element name="birth_history" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Raw memory map of the element</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="file_name" type="sciemetric:QW_filename" minOccurs="0" />
										<xs:element name="version_number" type="xs:float">
											<xs:annotation>
												<xs:documentation>This is the version on tag</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="version_id" type="sciemetric:QW_id" minOccurs="0" />
										<xs:element name="data" type="sciemetric:DataType" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="binary_catalog" minOccurs="0">
					<xs:annotation>
						<xs:documentation>This is the catalog of the binary file. This catalog will be used to decode the information in the binary block and can also be used as a stand alone element to replace the WFM file in the future</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence maxOccurs="unbounded">
							<xs:element name="item" type="sciemetric:CatalogItemsType" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="gateways" minOccurs="0">
					<xs:complexType>
						<xs:sequence maxOccurs="unbounded">
							<xs:element name="gateway" type="sciemetric:GatewayDataType" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="CatalogItemsType">
		<xs:annotation>
			<xs:documentation>Description of a binary block</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The waveform label that is linked to this catalog item.  This is for review purposes only.  If it does not match the waveform label, nothing bad will happen.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="reference" type="sciemetric:ReferenceNumberType">
				<xs:annotation>
					<xs:documentation>Reference number matching with the  one specified in the top section of the file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="sciemetric:QW_description" minOccurs="0" />
			<xs:element name="type" type="sciemetric:EnumDataType">
				<xs:annotation>
					<xs:documentation>Type of data stored in the binary block</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="index" type="xs:unsignedInt">
				<xs:annotation>
					<xs:documentation>This is the index order of this block</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="size">
				<xs:annotation>
					<xs:documentation>Size of the binary block</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="filesize" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="compression" type="sciemetric:EnumCompressionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Type of compression for the binary data</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="crc_check" type="xs:unsignedInt" minOccurs="0">
				<xs:annotation>
					<xs:documentation>CRC check of the binary block</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="data" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A series of x-y data pairs.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="x_label" type="sciemetric:QW_MdataLabel">
							<xs:annotation>
								<xs:documentation>The label used as the default label for the x axis on a graph.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="y_label" type="sciemetric:QW_MdataLabel">
							<xs:annotation>
								<xs:documentation>The label used as the default label for the y axis on a graph.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:sequence minOccurs="0">
							<xs:element name="x_start" type="xs:float">
								<xs:annotation>
									<xs:documentation>The first x point in a constant x interval waveform.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="x_interval" type="xs:float">
								<xs:annotation>
									<xs:documentation>The x interval in a constant x interval waveform</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
						<xs:element name="datapoint" type="sciemetric:DataPointType" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AnalysisType">
		<xs:annotation>
			<xs:documentation>Analysis region description</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="type" type="xs:short">
				<xs:annotation>
					<xs:documentation>This is the inspeXion signature enumeration</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="is_y_data" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>This let the user know if the waveform was a X or a Y format</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="range" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is the extraction range of the Analysis Region</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:choice maxOccurs="2">
							<xs:sequence>
								<xs:element name="start" type="xs:float">
									<xs:annotation>
										<xs:documentation>Startion point in Eng Units</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="format_start" type="sciemetric:QW_precision" minOccurs="0" />
							</xs:sequence>
							<xs:sequence>
								<xs:element name="stop" type="xs:float">
									<xs:annotation>
										<xs:documentation>Stopping point in Eng units</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="format_stop" type="sciemetric:QW_precision" minOccurs="0" />
							</xs:sequence>
						</xs:choice>
						<xs:element name="units" type="sciemetric:UnitType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Units of the value provided</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="limits" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Limit of the signature step</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice maxOccurs="2">
						<xs:sequence>
							<xs:element name="lower" type="xs:float">
								<xs:annotation>
									<xs:documentation>Lower limit</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="format_lower" type="sciemetric:QW_precision" minOccurs="0" />
						</xs:sequence>
						<xs:sequence>
							<xs:element name="upper" type="xs:float">
								<xs:annotation>
									<xs:documentation>Upper limit</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="format_upper" type="sciemetric:QW_precision" minOccurs="0" />
						</xs:sequence>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="envelope" type="sciemetric:EnvelopeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>In case of envelope, this is storage for it</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BuildComplexitiesType">
		<xs:annotation>
			<xs:documentation>Collection of BCs</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="build_complexity" type="sciemetric:BuildComplexityType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This is the definition of the complexity element</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ComponentComplexityType">
		<xs:annotation>
			<xs:documentation>Definition of the complexity</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" type="sciemetric:QW_name">
				<xs:annotation>
					<xs:documentation>Name of the complexity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" type="xs:integer" />
			<xs:element name="type_label" type="xs:string" />
			<xs:element name="type_number" type="xs:int" minOccurs="0" />
			<xs:element name="datestamp" type="sciemetric:QW_datetime" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BuildComplexityType">
		<xs:annotation>
			<xs:documentation>Definition of the complexity</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" type="sciemetric:QW_name">
				<xs:annotation>
					<xs:documentation>Name of the complexity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="data" type="xs:int">
				<xs:annotation>
					<xs:documentation>Value if the complexity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datestamp" type="sciemetric:QW_datetime" minOccurs="0" />
			<xs:element name="id" type="xs:integer" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ComponentType">
		<xs:annotation>
			<xs:documentation>Definition of a component</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" type="sciemetric:QW_name">
				<xs:annotation>
					<xs:documentation>Name of the component</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" type="sciemetric:QW_id" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ComponentComplexitiesType">
		<xs:annotation>
			<xs:documentation>Collection of CCs</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="component_complexity" type="sciemetric:ComponentComplexityType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Definition of the component complexity</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConnectionType">
		<xs:annotation>
			<xs:documentation>This is the information about the database connection</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="2">
			<xs:element name="unique_identifier" type="sciemetric:GUIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This unique identifier is used to ensure the synchronisation of the id with the database</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="DataHeaderType">
		<xs:annotation>
			<xs:documentation>Description of the data information </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="type" type="sciemetric:EnumDataType">
				<xs:annotation>
					<xs:documentation>This enum tell the type of data stored in this node</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="data_id" type="sciemetric:QW_id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The data_id value.  Not valid for all data types.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="format" type="sciemetric:QW_precision" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This will be the C like forrmat used to convert the data to string</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unit" type="sciemetric:UnitType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unit of the data</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DataPointType">
		<xs:annotation>
			<xs:documentation>An x-y data pair from a waveform</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="xdata" type="xs:float" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The x data value from an x-y data pair.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ydata" type="xs:float">
				<xs:annotation>
					<xs:documentation>The y data value from an x-y data pair.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="order" type="sciemetric:QW_order">
				<xs:annotation>
					<xs:documentation>The order value from an x-y data pair within a waveform.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DataType">
		<xs:annotation>
			<xs:documentation>Data storage</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="sciemetric:DataHeaderType">
				<xs:choice>
					<xs:element name="reference" type="sciemetric:ReferenceNumberType">
						<xs:annotation>
							<xs:documentation>In case of reference to the binary block, this number will be the link</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="value" type="xs:anyType">
						<xs:annotation>
							<xs:documentation>In case of local data, The value will be stored in this element. This element as not type check </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DefectType">
		<xs:annotation>
			<xs:documentation>Description of a defect</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" type="sciemetric:QW_name">
				<xs:annotation>
					<xs:documentation>Name of the defect</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" type="sciemetric:QW_id" minOccurs="0" />
			<xs:element name="data_id" type="sciemetric:QW_id" minOccurs="0" />
			<xs:element name="time_in" type="sciemetric:QW_datetime">
				<xs:annotation>
					<xs:documentation>Type the defect is flaged</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="time_repaired" type="sciemetric:QW_datetime">
				<xs:annotation>
					<xs:documentation>repaired time</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="true_defect" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>True defect of only a text???</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="location" type="sciemetric:LocationType">
				<xs:annotation>
					<xs:documentation>Location of of the defect</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="component" type="sciemetric:ComponentType">
				<xs:annotation>
					<xs:documentation>Defective component</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="repair_station" type="sciemetric:RepairStationType">
				<xs:annotation>
					<xs:documentation>Station involved for this reparation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operator" type="sciemetric:OperatorType">
				<xs:annotation>
					<xs:documentation>Repair Operator personnal information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="audit" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The defect audit flag.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rank" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The defect prediction rank value.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="repairs" type="sciemetric:RepairsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Repair stesp to make it work</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DefectsType">
		<xs:annotation>
			<xs:documentation>Collection of Defects</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="defect" type="sciemetric:DefectType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Definition of the defect</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EnvelopeType">
		<xs:annotation>
			<xs:documentation>Description of an envelope</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" type="sciemetric:QW_label" />
			<xs:element name="description" type="sciemetric:QW_description" minOccurs="0" />
			<xs:element name="id" type="sciemetric:QW_id" minOccurs="0" />
			<xs:element name="version" type="xs:integer" />
			<xs:element name="data" type="sciemetric:DataType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ElementDefinitionType">
		<xs:annotation>
			<xs:documentation>Base to define the general information on element</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" type="sciemetric:QW_label">
				<xs:annotation>
					<xs:documentation>Name of the element</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="sciemetric:QW_description" minOccurs="0" />
			<xs:element name="result" type="sciemetric:ResultType" />
			<xs:element name="order" type="sciemetric:QW_order" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Display order for the element</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" type="sciemetric:QW_id" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FeatureType">
		<xs:annotation>
			<xs:documentation>Dexscription of a feature</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="sciemetric:ElementDefinitionType">
				<xs:sequence>
					<xs:element name="date_stamp" type="sciemetric:QW_datetime">
						<xs:annotation>
							<xs:documentation>Date and tine of the element creation</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="data" type="sciemetric:DataType">
						<xs:annotation>
							<xs:documentation>Feature Data</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="analysis_region" type="sciemetric:AnalysisType" minOccurs="0">
