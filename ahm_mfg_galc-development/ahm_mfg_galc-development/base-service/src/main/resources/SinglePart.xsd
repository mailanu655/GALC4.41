								<xs:element name="x" type="sciemetric:WaveformDataType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>X nominal</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="y" type="sciemetric:WaveformDataType">
									<xs:annotation>
										<xs:documentation>Y nominal</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="features" type="sciemetric:FeaturesType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Collection of features associate with this waveform</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="WaveformsType">
		<xs:annotation>
			<xs:documentation>Collection of waveforms</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="waveform" type="sciemetric:WaveformType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Definition of the waveform</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="EnumDataSource" final="restriction">
