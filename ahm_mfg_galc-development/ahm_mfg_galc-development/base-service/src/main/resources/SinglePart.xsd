		</xs:annotation>
		<xs:restriction base="sciemetric:QW_StringFilterL">
			<xs:whiteSpace value="preserve" />
			<xs:minLength value="1" />
			<xs:maxLength value="12" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QW_Result_Label">
		<xs:annotation>
			<xs:documentation>Result label</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="preserve" />
			<xs:minLength value="1" />
			<xs:maxLength value="12" />
			<xs:pattern value="[0-9A-Za-z][\-\(\)/0-9A-Z\s\[\\\]_a-z]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QW_label50">
		<xs:annotation>
			<xs:documentation>Database label (Special with 50 chars for the binary catalog)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sciemetric:QW_StringFilterL">
			<xs:whiteSpace value="preserve" />
