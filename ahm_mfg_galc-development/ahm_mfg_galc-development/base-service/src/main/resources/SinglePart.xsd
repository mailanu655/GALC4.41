			<xs:documentation>Sciemetric String Filter for Serial and Model Numbers</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[\- \(\)\./0-9A-Z\[\\\]_a-z]*" />
		</xs:restriction>
	</xs:simpleType>
