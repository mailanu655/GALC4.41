<?xml version="1.0" encoding="UTF-8"?>
<connector id="Connector_ID" version="1.5" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/connector_1_5.xsd"> 
	<description></description>
	<display-name>jca-adaptor</display-name>
 	<vendor-name></vendor-name>
  	<eis-type>General Socket Server</eis-type>
  	<resourceadapter-version>1.5</resourceadapter-version>
  	                                  
  <resourceadapter id="ResourceAdapter1">
    <resourceadapter-class>com.honda.galc.jca.SocketResourceAdapter</resourceadapter-class>
    
    <!-- ResourceAdapter default configuration properties -->
    <!-- The inbound resource adapter description -->
    <inbound-resourceadapter>
      <messageadapter>
        <messagelistener>
          <messagelistener-type>com.honda.galc.jca.SocketMessageEndpoint</messagelistener-type>
          <activationspec>
            <activationspec-class>com.honda.galc.jca.SocketActivationSpec</activationspec-class>
          </activationspec>
        </messagelistener>
      </messageadapter>
    </inbound-resourceadapter>
  </resourceadapter>
</connector>
