<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<!-- Data Sources -->
	<data-sources>
	</data-sources>

	<!-- Form Beans -->
	<form-beans>
		<form-bean name="startFormBean" type="com.honda.galc.oif.web.forms.OifStartFormBean">
		</form-bean>
	</form-beans>

	<!-- Global Exceptions -->
	<global-exceptions>
	</global-exceptions>

	<!-- Global Forwards -->
	<global-forwards>
	</global-forwards>

	<!-- Action Mappings -->
	<action-mappings>
		<action path="/reschedule_events" type="com.honda.galc.oif.web.actions.RescheduleEventsAction">
			<forward name="result" path="./WEB-INF/page/refresh_result.jsp">
			</forward>
		</action>
		<action path="/start_req" type="com.honda.galc.oif.web.actions.ViewManualStartRequestAction">
			<forward name="result" path="WEB-INF/page/start_req.jsp">
			</forward>
		</action>
		<action path="/start" type="com.honda.galc.oif.web.actions.ManualStartAction" name="startFormBean">
			<forward name="result" path="/WEB-INF/page/start_result.jsp">
			</forward>
		</action>
		<action path="/index" type="org.apache.struts.actions.ForwardAction" parameter="WEB-INF/page/index.jsp">
		</action>
		<action path="/index" type="com.honda.galc.oif.web.actions.CheckAction">
			<forward name="success" path="/WEB-INF/page/index.jsp">
			</forward>
			<forward name="failure" path="/WEB-INF/page/disabled.jsp">
			</forward>
		</action>
		<action path="/configuration" type="com.honda.galc.oif.web.actions.ConfigurationAction">
			<forward name="result" path="/WEB-INF/page/configuration.jsp">
			</forward>
		</action>
		<action path="/distribution_configuration" type="com.honda.galc.oif.web.actions.ConfigurationAction">
			<forward name="result" path="/WEB-INF/page/distribution_configuration.jsp">
			</forward>
		</action>
		<action path="/schedule" type="org.apache.struts.actions.ForwardAction" parameter="WEB-INF/page/schedule.jsp">
		</action>
		<action path="/distribution_schedule" type="org.apache.struts.actions.ForwardAction" parameter="WEB-INF/page/distribution_schedule.jsp">
		</action>
		<action path="/logout" type="org.apache.struts.actions.ForwardAction" parameter="WEB-INF/page/logout.jsp">
		</action>
		<action path="/event_status" type="com.honda.oifserviceweb.actions.EventStatusAction">
			<forward name="result" path="/WEB-INF/page/event_status.jsp">
			</forward>
		</action>
		<action path="/ctl_schedule" type="com.honda.galc.oif.web.actions.ScheduleEventsAction">
			<forward name="result" path="./WEB-INF/page/refresh_result.jsp">
			</forward>
		</action>
		<action path="/distribution" type="org.apache.struts.actions.ForwardAction" parameter="WEB-INF/page/distribution.jsp">
		</action>
		<action path="/start_req_distribution" type="com.honda.galc.oif.web.actions.ViewManualStartDistributionRequestAction">
			<forward name="result" path="WEB-INF/page/start_req_distribution.jsp">
			</forward>
		</action>
		<action path="/start_distribution" type="com.honda.galc.oif.web.actions.ManualDistributionStartAction" name="startFormBean">
			<forward name="result" path="/WEB-INF/page/start_result_distribution.jsp">
			</forward>
		</action>
	</action-mappings>

	<!-- Message Resources -->
	<message-resources parameter="com.honda.galc.oif.web.resources.ApplicationResources"/>

</struts-config>
