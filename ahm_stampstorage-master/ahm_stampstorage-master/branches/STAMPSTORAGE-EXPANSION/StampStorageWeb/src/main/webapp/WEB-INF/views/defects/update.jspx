        <field:input field="yArea" label="Y Area (ex. A,B...)" id="c_com_honda_mfg_stamp_conveyor_domain_Defect_yArea"
                     required="true"
                     validationRegex="^[A-z]+$" z="xigUlTLRlOzmKdAfJuWFYTqMhsg="/>
        <field:input field="note" label="Note" id="c_com_honda_mfg_stamp_conveyor_domain_Defect_note"
                     required="false" z="xigUlTLRlOzmKdAfJuWFYTqMhsg="/>
        <field:checkbox field="defectRepaired" id="c_com_honda_mfg_stamp_conveyor_domain_Defect_defectRepaired"
                        z="gtx28PJLK8sOenpMvPyLvRRsPBk="/>
        <input type="hidden" name="detailedinventory" value="${detailedinventory}"/>
