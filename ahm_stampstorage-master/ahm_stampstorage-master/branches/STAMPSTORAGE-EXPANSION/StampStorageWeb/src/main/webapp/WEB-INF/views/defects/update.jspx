<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<div xmlns:field="urn:jsptagdir:/WEB-INF/tags/form/fields" xmlns:form="urn:jsptagdir:/WEB-INF/tags/form"
     xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0">
    <jsp:directive.page contentType="text/html;charset=UTF-8"/>
    <jsp:output omit-xml-declaration="yes"/>
    <form:update id="fu_com_honda_mfg_stamp_conveyor_domain_Defect" modelAttribute="defect" path="/defects"
                 versionField="Version" z="D2k2BgsiB6z5tB4E43JBfcTIlm4=">
        <field:input field="carrierNumber" id="c_com_honda_mfg_stamp_conveyor_domain_Defect_carrierNumber"
                     required="true" validationMessageCode="field_invalid_integer" z="koMPvbC7U58OwrlIuZWAZgeXjsU="/>
        <field:input field="productionRunNo" id="c_com_honda_mfg_stamp_conveyor_domain_Defect_productionRunNo"
                     required="true" validationMessageCode="field_invalid_integer" z="klnRYj6m69ZYO611aEebHSChhBc="/>
        <field:select field="defectType" id="c_com_honda_mfg_stamp_conveyor_domain_Defect_defectType"
                      items="${defect_types}" path="defect_types" required="true" z="AXiq93+l6P3rvnfyAkeVrFs59HY="/>
        <field:select field="reworkMethod" id="c_com_honda_mfg_stamp_conveyor_domain_Defect_reworkMethod"
                      items="${rework_methods}" path="rework_methods" required="true" z="z4UvMU9AGsCYJMarhT3cae0DxVY="/>
        <field:input field="xArea" label="X Area (ex.1, 2...)" id="c_com_honda_mfg_stamp_conveyor_domain_Defect_xArea"
                     required="true"
                     validationRegex="^[0-9]+$" validationMessageCode="field_invalid_integer"
