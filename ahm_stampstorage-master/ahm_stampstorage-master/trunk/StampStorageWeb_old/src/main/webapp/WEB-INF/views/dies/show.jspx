<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<div xmlns:c="http://java.sun.com/jsp/jstl/core"
     xmlns:field="urn:jsptagdir:/WEB-INF/tags/form/fields"
     xmlns:page="urn:jsptagdir:/WEB-INF/tags/form"
     xmlns:jsp="http://java.sun.com/JSP/Page"
     xmlns:security="http://www.springframework.org/security/tags"
     version="2.0">

    <jsp:directive.page contentType="text/html;charset=UTF-8"/>
    <jsp:output omit-xml-declaration="yes"/>
    <head>
        <script type="text/javascript">

            function changeHeaderBgColor(){
            var elem = document.getElementById("config");
            elem.style.backgroundColor = '#99CC66';
            }
            window.onload=changeHeaderBgColor;
        </script>
    </head>
    <c:set var="writable" value="false"/>

    <security:authorize ifAllGranted="ROLE_ADMIN">
        <c:set var="writable" value="true"/>
    </security:authorize>

    <page:show id="ps_com_honda_mfg_stamp_storage_domain_Die" object="${die}" path="/dies" z="user-managed"
               create="${writable}" update="${writable}" delete="${writable}">
        <field:display field="id" id="s_com_honda_mfg_stamp_storage_domain_Die_id" object="${die}"
                       z="user-managed"/>
        <field:display field="bpmPartNumber" id="s_com_honda_mfg_stamp_storage_domain_Die_bpmpartnumber" object="${die}"
                       z="user-managed"/>
        <field:display field="description" id="s_com_honda_mfg_stamp_storage_domain_Die_description" object="${die}"
                       z="user-managed"/>
        <field:display field="partProductionVolume" id="s_com_honda_mfg_stamp_storage_domain_Die_partproductionVolume"
                       object="${die}" z="user-managed"/>
        <field:display field="imageFileName" id="s_com_honda_mfg_stamp_storage_domain_Die_imagefileName" object="${die}"
                       z="user-managed"/>
        <field:display field="backgroundColor" id="c_com_honda_mfg_stamp_storage_domain_Die_backgroundColor" object="${die}"
                      z="user-managed"/>
        <field:display field="textColor" id="c_com_honda_mfg_stamp_storage_domain_Die_textColor" object="${die}"
                      z="user-managed"/>
        <field:display field="active" id="s_com_honda_mfg_stamp_storage_domain_Die_active" object="${die}"
                       z="user-managed"/>
    </page:show>
</div>
